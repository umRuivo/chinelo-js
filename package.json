{
  "name": "auto-routes-system",
  "version": "1.0.0",
  "type": "module",
  "description": "Sistema de auto routes para Express.js",
  "main": "server.js",
  "scripts": {
    "start": "bun run export-routes && bun server.js",
    "dev": "bun run export-routes && bun --hot server.js",
    "lint": "bun eslint . --fix",
    "export.routes": "node core/scripts/generate-export-routes.js"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "cors": "^2.8.5",
    "eslint": "^9.30.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "path": "^0.12.7",
    "prisma": "^6.11.1",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "globals": "^16.3.0",
    "nodemon": "^2.0.22"
  }
}